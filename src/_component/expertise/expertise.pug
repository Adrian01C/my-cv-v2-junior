-  var firstTitle = data.expertise.firstTitle;
include ../first-title-wrapper/first-title-wrapper

.column(span="row")
  h3.meet-me__another-title=data.expertise.story.title
.column(span="row")
  ul.expertise__list
    each item, key in data.expertise.story.itemList
      li.expertise__item
        span.expertise__icon(class=`icon-${item.icon}`)
        span.expertise__label=item.label
        span.expertise__count=key + 1
